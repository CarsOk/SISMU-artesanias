<tr>
  <td class="centrar">
      <%= link_to artesano.id %>
  </td>
  <td>
      <%= link_to artesano.nombre %>
  </td>
  <td>
      <%= link_to artesano.snombre %>
  </td>
  <td>
      <%= link_to artesano.apellido %>
  </td>
  <td>
      <%= link_to artesano.sapellido %>
  </td>
  <td id="o" class="centrar">
      <%= link_to artesano.tel%>
  </td id="aw">
  <td class="centrar">
    <%= link_to edit_artesano_url(artesano) do %>
      <%= image_tag("iconos/editar", class:"icon_accion") %>
    <% end %>
  </td>
  <td class="centrar">
    <%= button_to artesano_path(artesano), method: :delete, class: "btn_borrar", form: {data: {turbo_confirm: '¿Estás seguro?' }} do %>
      <%= image_tag("iconos/borrar", class:"icon_accion") %>
    <% end %>
  </td>
</tr>